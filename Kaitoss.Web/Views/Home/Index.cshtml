        @foreach (var blog in Model.Blog)
        {
        <div class="col-lg-4 col-md-6 col-12">
          <!-- Single News -->
          <div class="single-news">
            <div class="image">
              <a href="javascript:void(0)"><img class="thumb" src="@blog.ImageUrl" alt="Blog" /></a>
              <div class="meta-details">
                <img class="thumb" src="~/assets/images/blog/b6.jpg" alt="Author" />
                <span>@blog.Writer</span>
              </div>
            </div>
            <div class="content-body">
              <h4 class="title">
                <a href="javascript:void(0)">@blog.Title</a>
              </h4>
              <p>
              @blog.Description
              </p>
            </div>
          </div>
          <!-- End Single News -->
        </div>}
      
