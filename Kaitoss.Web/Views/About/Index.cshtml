

@{
    ViewBag.Title = "Table Abouts";
    Layout = "~/Views/Shared/_dashboard.cshtml";
}
<div class="container p-3">
       <div class="row pt-4">
        <div class="col-12">
            <h1 class="h1-header mt-5"> Table Abouts</h1>
        </div>
        <div class="col-6 my-2">
            <a class="btn-primary btn"  asp-controller="About" asp-action="Upsert"><i class="bi bi-plus-circle me-1"></i> Create New About</a>
        </div>
    </div>
</div>

<table id="tblData" class="table table-hover table-bordered ">

    <thead>
        <tr>
            <th>
                Image 
            </th>
              <th>
                Title 
            </th>
            <th>
                Who we are
            </th>
            <th>
                Vision
            </th>
            <th>
               History
            </th>
            <th>
                Actions
            </th>
        </tr>
    </thead>
</table>
 
@section Scripts{
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="//cdn.datatables.net/1.12.1/js/jquery.dataTables.min.js"></script>
    <script src="~/Components/js/About.js"></script>
}
    

